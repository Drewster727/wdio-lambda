{
  "name": "wdio-lambda",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "scripts": {
    "build": "webpack",
    "debug": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/serverless offline -s dev",
    "package": "rm -rf node_modules && docker run -v $PWD:/var/task -w /var/task --entrypoint='sh' lambci/lambda:build-nodejs8.10 package.sh && rm -rf ~/Desktop/sls/* && cp -R .serverless/* ~/Desktop/sls/ && serverless deploy --package ~/Desktop/sls --force --aws-s3-accelerate",
    "deploy": "serverless deploy --force --aws-s3-accelerate"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.1.1",
    "serverless": "^1.60.4",
    "serverless-offline": "^5.12.1",
